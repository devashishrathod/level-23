{
  "info": {
    "name": "Level-23 CostSheets APIs",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "CostSheets - Create (Bulk)",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" },
          { "key": "Authorization", "value": "Bearer {{token}}" }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"projectId\": \"{{projectId}}\",\n  \"basicRate\": 16000,\n  \"development\": 500,\n  \"dgBackup\": 200,\n  \"recreation\": 200,\n  \"societyLegal\": 100,\n  \"floorRise\": 50,\n  \"otherCharges\": 1000000\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/costSheets/create",
          "host": ["{{baseUrl}}"],
          "path": ["costSheets", "create"]
        }
      }
    },
    {
      "name": "CostSheets - GetAll (Admin)",
      "request": {
        "method": "GET",
        "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
        "url": {
          "raw": "{{baseUrl}}/costSheets/getAll?page=1&limit=10",
          "host": ["{{baseUrl}}"],
          "path": ["costSheets", "getAll"],
          "query": [
            { "key": "page", "value": "1" },
            { "key": "limit", "value": "10" }
          ]
        }
      }
    },
    {
      "name": "CostSheets - GetAll For User",
      "request": {
        "method": "GET",
        "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
        "url": {
          "raw": "{{baseUrl}}/costSheets/getAllForUser?page=1&limit=10",
          "host": ["{{baseUrl}}"],
          "path": ["costSheets", "getAllForUser"],
          "query": [
            { "key": "page", "value": "1" },
            { "key": "limit", "value": "10" }
          ]
        }
      }
    },
    {
      "name": "CostSheets - Get By Id",
      "request": {
        "method": "GET",
        "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
        "url": {
          "raw": "{{baseUrl}}/costSheets/get/{{costSheetId}}",
          "host": ["{{baseUrl}}"],
          "path": ["costSheets", "get", "{{costSheetId}}"]
        }
      }
    },
    {
      "name": "CostSheets - Update",
      "request": {
        "method": "PUT",
        "header": [
          { "key": "Content-Type", "value": "application/json" },
          { "key": "Authorization", "value": "Bearer {{token}}" }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"basicRate\": 16500\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/costSheets/update/{{costSheetId}}",
          "host": ["{{baseUrl}}"],
          "path": ["costSheets", "update", "{{costSheetId}}"]
        }
      }
    },
    {
      "name": "CostSheets - Delete",
      "request": {
        "method": "DELETE",
        "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
        "url": {
          "raw": "{{baseUrl}}/costSheets/delete/{{costSheetId}}",
          "host": ["{{baseUrl}}"],
          "path": ["costSheets", "delete", "{{costSheetId}}"]
        }
      }
    }
  ],
  "variable": [
    { "key": "baseUrl", "value": "http://localhost:8500/level-23" },
    { "key": "token", "value": "" },
    { "key": "projectId", "value": "" },
    { "key": "towerId", "value": "" },
    { "key": "floorId", "value": "" },
    { "key": "unitId", "value": "" },
    { "key": "costSheetId", "value": "" }
  ]
}
